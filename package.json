{
  "name": "wturbo",
  "version": "1.0.1",
  "main": "dist/index.js",
  "bin": {
    "wturbo": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "biome lint src/ e2e/",
    "format": "biome format --write src/ e2e/",
    "check": "biome check --write src/ e2e/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run src/",
    "test:e2e": "vitest run e2e/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "typescript",
    "commander",
    "git",
    "worktree",
    "docker",
    "compose"
  ],
  "author": "",
  "license": "ISC",
  "description": "Git worktree management with Docker Compose environment isolation",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.4.0",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "fs-extra": "^11.3.1",
    "yaml": "^2.8.1"
  }
}

{
  "name": "wtcompose",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": {
    "wtcompose": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [
    "cli",
    "typescript",
    "commander"
  ],
  "author": "",
  "license": "ISC",
  "description": "CLI tool built with TypeScript and Commander.js",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.4.0",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "fs-extra": "^11.3.1",
    "simple-git": "^3.28.0",
    "yaml": "^2.8.1"
  }
}
